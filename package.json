    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.0.6",
    "postcss-clean": "^1.1.0",
    "postcss-loader": "^2.1.1",
    "postcss": "^6.0.19",
    "purgecss-webpack-plugin": "^1.4.0",
    "style-loader": "^0.20.3",
