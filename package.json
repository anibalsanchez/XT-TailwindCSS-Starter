{
  "name": "xt-tailwindcss-starter",
  "version": "4.0.0",
  "description": "A Tailwind CSS Starter. Based on Tailwind CSS, Webpack, PostCSS, cssnano and purgecss. Fully optimized for top performance.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "serve": "cross-env NODE_ENV=development webpack-dev-server",
    "dev-build": "cross-env NODE_ENV=development webpack",
    "prod": "cross-env NODE_ENV=production webpack --no-progress"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:anibalsanchez/XT-TailwindCSS-Starter.git"
  },
  "keywords": [
    "Tailwind, CSS, Webpack, PostCSS, cssnano, purgecss"
  ],
  "author": "Extly CB",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "@tailwindcss/ui": "^0.7.2",
    "autoprefixer": "^10.2.3",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "postcss": "^8.2.4",
    "postcss-clean": "^1.2.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-nested": "^5.0.3",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
